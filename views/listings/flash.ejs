<% if("success" && success.length){ %>
<div
  class="alert alert-success alert-dismissible offset-3 col-6 mt-2"
  role="alert"
>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if("failure" && failure.length ){ %>
<div
  class="alert offset-2 col-6 alert-danger alert-dismissible mt-2"
  role="alert"
>
  <%= failure %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
